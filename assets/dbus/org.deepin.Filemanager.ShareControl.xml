<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.deepin.Filemanager.UserShareManager">
    <method name="CloseSmbShareByShareName">
      <arg type="b" direction="out"/>
      <arg name="name" type="s" direction="in"/>
      <arg name="show" type="b" direction="in"/>
    </method>
    <method name="SetUserSharePassword">
      <arg type="b" direction="out"/>
      <arg name="name" type="s" direction="in"/>
      <arg name="passwd" type="s" direction="in"/>
    </method>
    <method name="EnableSmbServices">
      <arg type="b" direction="out"/>
    </method>
    <method name="IsUserSharePasswordSet">
      <arg type="b" direction="out"/>
      <arg name="username" type="s" direction="in"/>
    </method>
  </interface>
</node>
